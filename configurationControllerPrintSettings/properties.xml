    <className>ru.smcsystem.modules.configurationControllerPrintSettings.PrintSettings</className>
    <ver>1.2.0</ver>
    <apiVer>1.4.3</apiVer>
    <date>13-02-2026</date>

    <types>
        <type t="default" minC="1" maxC="-1">
            <description>
                <entry>
                    <key>ru</key>
                    <value>Выводит все настройки всех управляемых конфигураций.</value>
                </entry>
                <entry>
                    <key>en</key>
                    <value>Displays all settings for all managed configurations.</value>
                </entry>
            </description>
            <sources/>
            <output>
                <element t="VALUE" vt="INTEGER" v="1">
                    <description>
                        <entry k="ru" v="Количество конфигураций"/>
                        <entry k="en" v="Configurations count"/>
                    </description>
                </element>
                <element t="FOR">
                    <countValueId>-1</countValueId>
                    <elements>
                        <element t="VALUE" o="true" vt="STRING" v="cfg1">
                            <description>
                                <entry k="ru" v="Имя конфигурации"/>
                                <entry k="en" v="Configuration name"/>
                            </description>
                        </element>
                        <element t="VALUE" o="true" vt="INTEGER" v="1">
                            <description>
                                <entry k="ru" v="Количество настроек"/>
                                <entry k="en" v="Settings count"/>
                            </description>
                        </element>
                        <element t="VALUE" o="true" vt="STRING" v="s1">
                            <description>
                                <entry k="ru" v="Имя настройки"/>
                                <entry k="en" v="Setting name"/>
                            </description>
                        </element>
                        <element t="VALUE" o="true" vt="STRING" v="cfg1">
                            <description>
                                <entry k="ru" v="Значение настройки"/>
                                <entry k="en" v="Setting value"/>
                            </description>
                        </element>
                    </elements>
                </element>
            </output>
        </type>
        <type t="setting" minS="1" maxS="1" minC="1" maxC="-1">
            <description>
                <entry>
                    <key>ru</key>
                    <value>Выводит значения указанных настроек из всех управляемых конфигураций.</value>
                </entry>
                <entry>
                    <key>en</key>
                    <value>Displays the values of the specified settings from all managed configurations.</value>
                </entry>
            </description>
            <sources>
                <source>
                    <count>1</count>
                    <elements>
                        <element t="VALUE" c="-1" vt="STRING" v="s1">
                            <description>
                                <entry k="ru" v="Имя настройки"/>
                                <entry k="en" v="Setting name"/>
                            </description>
                        </element>
                    </elements>
                </source>
            </sources>
            <output>
                <element t="VALUE_ANY" c="-1" vt="STRING" v="cfg1">
                    <description>
                        <entry k="ru" v="Значение настройки"/>
                        <entry k="en" v="Setting value"/>
                    </description>
                </element>
            </output>
        </type>
        <type t="variable" minS="1" maxS="1" minC="1" maxC="-1">
            <description>
                <entry>
                    <key>ru</key>
                    <value>Выводит значения указанных переменных из всех управляемых конфигураций.</value>
                </entry>
                <entry>
                    <key>en</key>
                    <value>Displays the values of the specified variables from all managed configurations.</value>
                </entry>
            </description>
            <sources>
                <source>
                    <count>1</count>
                    <elements>
                        <element t="VALUE" c="-1" vt="STRING" v="v1">
                            <description>
                                <entry k="ru" v="Имя переменной"/>
                                <entry k="en" v="Variable name"/>
                            </description>
                        </element>
                    </elements>
                </source>
            </sources>
            <output>
                <element t="VALUE_ANY" c="-1" vt="STRING" v="value1">
                    <description>
                        <entry k="ru" v="Значение настройки"/>
                        <entry k="en" v="Variable value"/>
                    </description>
                </element>
            </output>
        </type>
    </types>

    <description>
        <entry>
            <key>en</key>
            <value>The module displays all the settings.</value>
        </entry>
        <entry>
            <key>ru</key>
            <value>Модуль выводит все настройки.</value>
        </entry>
    </description>

    <help>
        <entry>
            <key>en</key>
            <value><![CDATA[The module displays all settings.
The data is listed in the following order: the managed configuration number, followed by the parameter name and parameter value.
It can also display only the values of specified settings or variables.
            ]]></value>
        </entry>
        <entry>
            <key>ru</key>
            <value><![CDATA[Модуль выводит все настройки.
Данные идут в порядке: порядковый номер управляемой конфигурации, и по порядку имя параметра, значение параметра.
Так же способен выводить только значения указанных настроек или переменных.
            ]]></value>
        </entry>
    </help>

    <examples>
        <example>
            <description>
                <entry>
                    <key>en</key>
                    <value>The managed configuration has two settings: "setting1" = "value1", "setting2" = "value2".</value>
                </entry>
                <entry>
                    <key>ru</key>
                    <value>Управляемая конфигурация имеет две настройки: "setting1"="value1", "setting2"="value2".</value>
                </entry>
            </description>
            <settings/>
            <variables/>
            <sources/>
            <output>
                <message>
                    <type>STRING</type>
                    <value>setting1</value>
                </message>
                <message>
                    <type>STRING</type>
                    <value>value2</value>
                </message>
                <message>
                    <type>STRING</type>
                    <value>setting2</value>
                </message>
                <message>
                    <type>STRING</type>
                    <value>value2</value>
                </message>
            </output>
            <countManagedExecutionContexts>0</countManagedExecutionContexts>
            <countManagedConfigurations>1</countManagedConfigurations>
        </example>
    </examples>

</properties>
